{
    "name": "wkii/yii2-adminlte",
    "description": "AdminLTE theme for Yii2 Framework",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","adminlte"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "terry",
            "email": "digihero@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-bootstrap": "2.*",
        "almasaeed2010/adminlte": "~2.0",
        "bower-asset/fontawesome": ">=4.6.3",
        "bower-asset/pace": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Wkii\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
